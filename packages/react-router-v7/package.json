{
  "name": "@url-query-filters/react-router-v7",
  "version": "0.0.1",
  "author": "RyDVi",
  "description": "React Router v7 adapter for url-query-filters. url-query-filters is designed for convenient parsing, serialization, and management of filters in the URL query string.",
  "homepage": "https://github.com/RyDVi/url-query-filters",
  "type": "module",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist && vite build",
    "types": "tsc -p tsconfig.json",
    "types:watch": "tsc -p tsconfig.json --watch"
  },
  "dependencies": {
    "@url-query-filters/common": "workspace:*"
  },
  "devDependencies": {
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0 || ^19.0.0",
    "react-router-dom": "^7.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
