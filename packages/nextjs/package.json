  },
  "dependencies": {
    "@url-query-filters/common": "workspace:*"
  },
  "devDependencies": {
    "@types/react": "^18.3.23"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0 || ^19.0.0",
    "next": "^12.0.0 || ^13.0.0 || ^14.0.0"
