{
  "name": "@url-query-filters/nextjs",
  "version": "0.0.0",
  "author": "RyDVi",
  "description": "Next.js adapter for url-query-filters. url-query-filters is designed for convenient parsing, serialization, and management of filters in the URL query string.",
  "homepage": "https://github.com/RyDVi/url-query-filters",
  "type": "module",
  "main": "dist/index.js",
