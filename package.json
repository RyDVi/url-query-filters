{
  "name": "url-query-filters",
  "version": "0.0.1",
  "author": "RyDVi",
  "description": "url-query-filters is designed for convenient parsing, serialization, and management of filters in the URL query string.",
  "homepage": "https://github.com/RyDVi/url-query-filters",
  "type": "module",
  "workspaces": [
    "packages/*",
    "playground/*"
  ],
  "scripts": {
    "build": "pnpm -r run build",
    "types": "pnpm -r run types",
    "copy-readme": "for d in packages/*; do if [ -d \"$d\" ]; then cp README.md \"$d/README.md\"; fi; done"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.4",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
